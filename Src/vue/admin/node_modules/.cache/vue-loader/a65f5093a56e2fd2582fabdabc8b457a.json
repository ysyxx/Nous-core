, callback) => {\r\n\t\t\tif(!value){\r\n\t\t\t\tcallback();\r\n\t\t\t} else if (!isIntNumer(value)) {\r\n\t\t\t\tcallback(new Error(\"请输入整数\"));\r\n\t\t\t} else {\r\n\t\t\t\tcallback();\r\n\t\t\t}\r\n\t\t};\r\n\t\treturn {\r\n\t\t\tid: '',\r\n\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\tro:{\r\n\t\t\t\tdongwunicheng : false,\r\n\t\t\t\tdongwuzhonglei : false,\r\n\t\t\t\tpinzhong : false,\r\n\t\t\t\tdongwutupian : false,\r\n\t\t\t\txingbie : false,\r\n\t\t\t\tdongwunianling : false,\r\n\t\t\t\txingqing : false,\r\n\t\t\t\tzhongliang : false,\r\n\t\t\t\tzhuangtai : false,\r\n\t\t\t\tjianjie : false,\r\n\t\t\t\tfaburiqi : false,\r\n\t\t\t\tclicktime : false,\r\n\t\t\t\tclicknum : false,\r\n\t\t\t\tdiscussnum : false,\r\n\t\t\t\tstoreupnum : false,\r\n\t\t\t},\r\n\t\t\t\r\n\t\t\t\r\n\t\t\truleForm: {\r\n\t\t\t\tdongwunicheng: '',\r\n\t\t\t\tdongwuzhonglei: '',\r\n\t\t\t\tpinzhong: '',\r\n\t\t\t\tdongwutupian: '',\r\n\t\t\t\txingbie: '',\r\n\t\t\t\tdongwunianling: '',\r\n\t\t\t\txingqing: '',\r\n\t\t\t\tzhongliang: '',\r\n\t\t\t\tzhuangtai: '待领养',\r\n\t\t\t\tjianjie: '',\r\n\t\t\t\tfaburiqi: '',\r\n\t\t\t\tclicktime: '',\r\n\t\t\t},\r\n\t\t\r\n\t\t\tdongwuzhongleiOptions: [],\r\n\t\t\txingbieOptions: [],\r\n\t\t\tzhuangtaiOptions: [],\r\n\r\n\t\t\t\r\n\t\t\trules: {\r\n\t\t\t\tdongwunicheng: [\r\n\t\t\t\t\t{ required: true, message: '动物昵称不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tdongwuzhonglei: [\r\n\t\t\t\t\t{ required: true, message: '动物种类不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tpinzhong: [\r\n\t\t\t\t],\r\n\t\t\t\tdongwutupian: [\r\n\t\t\t\t],\r\n\t\t\t\txingbie: [\r\n\t\t\t\t\t{ required: true, message: '性别不能为空', trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\tdongwunianling: [\r\n\t\t\t\t\t{ required: true, message: '动物年龄不能为空', trigger: 'blur' },\r\n\t\t\t\t\t{ validator: validateIntNumber, trigger: 'blur' },\r\n\t\t\t\t],\r\n\t\t\t\txingqing: [\r\n\t\t\t\t],\r\n\t\t\t\tzhongliang: [\r\n\t\t\t\t\t{ validator: validateNumber, trigger: 'blur' },\r\n\t\